# % returns all mailboxes matching

C: a0 LIST "" ""
S: \* LIST \(\\Noselect\) \"\.\" \"\"
S: a0 OK LIST completed.

C: a1 LIST "#private" "%"
SUB {
S: \* LIST \(\\HasChildren\) \"\.\" \"#private.Boby.sharedMailbox\"
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.imapuser.INBOX\"
}
S: a1 OK LIST completed.

C: a11 LIST "#private" %
SUB {
S: \* LIST \(\\HasChildren\) \"\.\" \"#private.Boby.sharedMailbox\"
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.imapuser.INBOX\"
}
S: a11 OK LIST completed.

C: a11 LIST "#private" %.%
SUB {
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.Boby.sharedMailbox.child\"
}
S: a11 OK LIST completed.

C: a12 LIST "#private" %.*
SUB {
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.Boby.sharedMailbox.child\"
}
S: a12 OK LIST completed.

C: a2 LIST "#private" "*"
SUB {
S: \* LIST \(\\HasChildren\) \"\.\" \"#private.Boby.sharedMailbox\"
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.Boby.sharedMailbox.child\"
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.imapuser.INBOX\"
}
S: a2 OK LIST completed.

C: a3 LIST "#private" sharedMailbox*
SUB {
S: \* LIST \(\\HasChildren\) \"\.\" \"#private.Boby.sharedMailbox\"
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.Boby.sharedMailbox.child\"
}
S: a3 OK LIST completed.

C: a31 LIST "#private" "sharedMailbox.%"
SUB {
S: \* LIST \(\\HasNoChildren\) \"\.\" \"#private.Boby.sharedMailbox.child\"
}
S: a31 OK LIST completed.

#C: a002 GETACL #private.Boby.sharedMailbox
#S: \* ACL user.Boby boby r imapuser a
#S: a002 OK Completed

#C: a001 MYRIGHTS #private.Boby.sharedMailbox
#S: \* MYRIGHTS user.Boby r
#S: a001 OK Completed
